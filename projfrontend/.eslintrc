{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["wesbos"],
  "rules": {
    "no-console": 2,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 4,
        "endOfLine":"auto"
      }
    ]
  }
  
}
